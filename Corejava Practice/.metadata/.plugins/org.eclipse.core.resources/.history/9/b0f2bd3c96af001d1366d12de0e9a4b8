package com.FailsafeandFailFast;

import java.util.Iterator;
import java.util.concurrent.CopyOnWriteArrayList;
import java.util.concurrent.CopyOnWriteArraySet;

public class Failsafe {
	
	public static void main(String[] args) {
		
		System.out.println("For LIST.....");
		CopyOnWriteArrayList<Integer>al=new CopyOnWriteArrayList<>();
		al.add(10);
		al.add(20);
		al.add(30);
		
		     Iterator<Integer> itr  =  al.iterator();
		     while(itr.hasNext()) {
		    		al.add(90);
		    	Integer i= itr.next();
		    
		    	System.out.println(i);
		    	System.out.println(al);
		     }
		
		     
		CopyOnWriteArraySet<Integer>al1=new CopyOnWriteArraySet<>();
		al1.add(10);
		al1.add(20);
		al1.add(30);
		
		              Iterator<Integer>itr1     = al1.iterator();
		                 while(itr1.hasNext()) {
		                	Integer i= itr.next();
		                	al.add(80);
		                	System.out.println(i);
		                	System.out.println(al);
		                 }
		
		
		
		
		
		
		
		
		
	
	}

}
